  "tokenColors": [
    { "scope": "comment", "settings": { "foreground": "#352f3d99" } },
    {
      "scope": ["comment.line", "comment.block.documentation"],
      "settings": { "foreground": "#352f3d99", "fontStyle": "italic" }
    },
    {
      "scope": "constant.language.boolean.false",
      "settings": { "foreground": "#993333" }
    },
    {
      "scope": "constant.language.boolean.true",
      "settings": { "foreground": "#993333" }
    },
    {
      "scope": "constant.language.import-export-all",
      "settings": { "foreground": "#2d6a9f" }
    },
    {
      "scope": "constant.language.null",
      "settings": { "foreground": "#993333" }
    },
    {
