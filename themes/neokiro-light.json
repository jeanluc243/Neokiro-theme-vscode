    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": { "foreground": "#dbb90f" }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#d08025" }
    },
    { "scope": "keyword", "settings": { "foreground": "#876eb1" } },
    {
      "scope": [
        "keyword.control",
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.flow"
      ],
      "settings": { "foreground": "#7559a6" }
    },
    {
      "scope": ["keyword.operator", "keyword.operator.logical"],
      "settings": { "foreground": "#352f3dcc" }
    },
    {
      "scope": ["keyword.unit", "keyword.other.unit"],
      "settings": { "foreground": "#e21067" }
    },
    { "scope": ["markup.bold"], "settings": { "foreground": "#7559a6" } },
    {
      "scope": ["markup.italic"],
      "settings": { "foreground": "#2d6a9f", "fontStyle": "italic" }
    },
    {
      "scope": ["markup.fenced_code"],
      "settings": { "foreground": "#352f3dcc" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#352f3dcc", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.quote"],
      "settings": { "foreground": "#352f3dcc", "fontStyle": "italic" }
    },
    {
      "scope": ["markup.underline_link"],
      "settings": { "foreground": "#0c9aa7" }
    },
    { "scope": ["meta.class"], "settings": { "foreground": "#c80e5c" } },
    {
      "scope": ["meta.jsx.children"],
      "settings": { "foreground": "#352f3dcc" }
    },
    { "scope": ["meta.object"], "settings": { "foreground": "#0c9aa7" } },
    {
      "scope": ["meta.property-name.css"],
      "settings": { "foreground": "#2b36ab" }
    },
    {
      "scope": ["meta.property-value.css"],
      "settings": { "foreground": "#2b36ab" }
    },
    { "scope": ["meta.selector"], "settings": { "foreground": "#367c53" } },
    {
      "scope": ["meta.structure.dictionary"],
      "settings": { "foreground": "#2b36ab" }
    },
    {
      "scope": ["meta.tag.attributes"],
      "settings": { "foreground": "#0c9aa7" }
    },
    {
      "scope": ["meta.type.parameters"],
      "settings": { "foreground": "#303cc1" }
    },
    { "scope": ["meta.var.expr"], "settings": { "foreground": "#303cc1" } },
    {
      "scope": [
        "punctuation.accessor",
        "punctuation.definition.array",
        "punctuation.definition.block",
        "punctuation.definition.dictionary",
        "punctuation.definition.parameters",
        "punctuation.definition.tag",
        "punctuation.definition.typeparameters",
        "punctuation.separator"
      ],
      "settings": { "foreground": "#352f3dcc" }
    },
    {
      "scope": ["punctuation.definition.heading"],
      "settings": { "foreground": "#d08025" }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": { "foreground": "#367c53" }
    },
    {
      "scope": ["punctuation.definition.template-expression"],
      "settings": { "foreground": "#993333" }
    },
    {
      "scope": ["punctuation.selection", "punctuation.terminator"],
      "settings": { "foreground": "#352f3d99" }
    },
    { "scope": ["storage"], "settings": { "foreground": "#7559a6" } },
    { "scope": ["string"], "settings": { "foreground": "#367c53" } },
    {
      "scope": ["support.class", "support.class.component"],
      "settings": { "foreground": "#c80e5c" }
    },
    {
      "scope": [
        "support.constant.color",
        "support.constant.font-name",
        "support.constant.property-value"
      ],
      "settings": { "foreground": "#3277b3" }
    },
    { "scope": ["support.function"], "settings": { "foreground": "#303cc1" } },
    {
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#876eb1" }
    },
    {
      "scope": ["support.type.property-name.css"],
      "settings": { "foreground": "#352f3dcc" }
    },
    {
      "scope": ["support.type.vendored"],
      "settings": { "foreground": "#dbb90f" }
    },
    { "scope": ["variable"], "settings": { "foreground": "#0c9aa7" } },
    {
      "scope": ["variable.language.super"],
      "settings": { "foreground": "#3d8c5e", "fontStyle": "italic" }
    },
    {
      "scope": ["variable.language.this"],
      "settings": { "foreground": "#0dadbc", "fontStyle": "italic" }
    }
  ]
